{% extends 'base.html.twig' %}

{% block title %}Todas las tareas{% endblock %}

{% block body %}

    <div class="example-wrapper">
        <h2>{{ task.title }}</h2>
        <p class="data-task">
            {{ task.user.name }} {{ task.user.surname }} | {{ task.createAt|date('d/m/Y h:i:s') }}
        </p>
        <p>
            {{ task.content }}
        </p>
    </div>
{% endblock %}
